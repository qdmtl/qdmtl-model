                                                                        owl:onProperty ecrm:P101_had_as_general_use ;
                                                                        owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                                        owl:onClass qdmtl:BuildingUsageType
                                                                      ]
                                                                    ) ;
                                                 rdf:type owl:Class
                                               ] ;
